<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.appbyrxjava20.MainActivity">

    <Button
        android:id="@+id/btn_test01ObservableAndObserver"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="使用RXJava2--ObservableAndObserver" />

    <Button
        android:id="@+id/btn_test02Disposable"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="使用RXJava2--Disposable" />

    <Button
        android:id="@+id/btn_test03Consumer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="使用RXJava2--Consumer" />

    <Button
        android:id="@+id/btn_test04Scheduler"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="使用RXJava2--Scheduler" />

    <Button
        android:id="@+id/btn_test05Flowable"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="使用RXJava2.0--Flowable" />

    <Button
        android:id="@+id/btn_test06Single"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="使用RXJava2.0--Single" />

    <Button
        android:id="@+id/btn_test07Completable"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="使用RXJava2.0--Completable" />

    <Button
        android:id="@+id/btn_test08_Subject"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="使用RXJava2.0--Subject" />

    <Button
        android:id="@+id/btn_test09_Processor"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="使用RXJava2.0--Processor" />

</LinearLayout>
